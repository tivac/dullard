{
  "name": "dullard",
  "version": "4.0.2",
  "description": "Let the computers do the boring stuff.",
  "bin": "./bin/cli.js",
  "main": "./lib/build.js",
  "scripts": {
    "commitmsg": "validate-commit-msg",
    "cover": "nyc jest --cache",
    "lint": "eslint .",
    "preview": "changes || true",
    "test": "jest",
    "posttest": "npm run lint",
    "preversion": "npm test",
    "version": "changes",
    "postversion": "git push --follow-tags",
    "watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tivac/dullard.git"
  },
  "author": "Pat Cavit <npm@patcavit.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tivac/dullard/issues"
  },
  "devDependencies": {
    "@studio/changes": "^1.1.0",
    "calmcard": "^0.1.1",
    "cli-tester": "^2.0.0",
    "dedent": "^0.7.0",
    "eslint": "^3.7.0",
    "eslint-config-arenanet": "^3.0.0",
    "husky": "^0.12.0",
    "jest": "^19.0.2",
    "nyc": "^10.2.0",
    "validate-commit-msg": "^2.8.2"
  },
  "dependencies": {
    "glob": "^7.0.0",
    "humanize-duration": "^3.9.1",
    "is-promise": "^2.1.0",
    "lodash.mergewith": "^4.6.0",
    "lodash.omit": "^4.5.0",
    "lodash.values": "^4.3.0",
    "meow": "^3.7.0",
    "npmlog": "^4.0.2",
    "p-each-series": "^1.0.0",
    "strip-json-comments": "^2.0.1",
    "update-notifier": "^2.1.0",
    "uppity": "~0.1.1"
  },
  "eslintConfig": {
    "extends": "arenanet",
    "env": {
      "node": true,
      "jest": true
    }
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
